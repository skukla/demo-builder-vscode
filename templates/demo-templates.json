{
  "$schema": "./demo-templates.schema.json",
  "version": "1.0.0",
  "templates": [
    {
      "id": "citisignal",
      "name": "CitiSignal Headless Storefront",
      "description": "Next.js headless storefront with Adobe Commerce API Mesh integration",
      "icon": "nextjs",
      "featured": true,
      "tags": ["headless", "nextjs", "storefront"],
      "defaults": {
        "frontend": "citisignal-nextjs",
        "backend": "adobe-commerce-paas",
        "dependencies": ["commerce-mesh", "demo-inspector"],
        "configDefaults": {
          "ADOBE_COMMERCE_WEBSITE_CODE": "citisignal",
          "ADOBE_COMMERCE_STORE_CODE": "citisignal_store",
          "ADOBE_COMMERCE_STORE_VIEW_CODE": "citisignal_us"
        }
      }
    },
    {
      "id": "citisignal-eds",
      "name": "CitiSignal EDS Experience",
      "description": "Edge Delivery Services storefront with Commerce Drop-in components",
      "icon": "eds",
      "featured": true,
      "tags": ["eds", "edge-delivery", "dropin", "citisignal"],
      "defaults": {
        "frontend": "eds-citisignal-storefront",
        "backend": "adobe-commerce-accs",
        "dependencies": ["demo-inspector"]
      }
    }
  ]
}
